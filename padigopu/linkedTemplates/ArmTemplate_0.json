		"LS_Abhi_Src_Demo_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Abhi_Src_Demo'"
		},
		"LS_Abhi_Src_Demo_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_Abhi_Src_Demo'"
		},
		"LS_Ranisri_Source_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Ranisri_Source'"
		},
		"LS_Ranisri_Source_password": {
			"type": "secureString",
