		"LS_Test_Azure_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Test_Azure'"
		},
		"LS_gowri_dest_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_gowri_dest'"
